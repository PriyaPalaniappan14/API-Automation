    "required": [
        "analysisOutput",
        "assetType",
        "created",
        "creationId",
        "failureCode",
        "id",
        "ingestMessage",
        "ingestState",
        "jobDetails",
        "jobLogUrl",
        "metadata",
        "ownerId",
        "ownerName",
        "poolId",
        "processorId",
        "requestDetails",
        "sourceId",
        "sourceUrl",
        "storeLocation1",
        "storeLocation2",
        "storeLocation3",
        "storeLocation4",
        "thumbnailUrl",
        "transcodedDate",
        "transcodingProfileUrl"
    ]
}