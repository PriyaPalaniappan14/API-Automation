            "required": [
                "convParams.EMSG_TIMED_METADATA_BASE64",
                "convParams.ID3_TIMED_METADATA_BASE64",
                "notifyFailureUrl",
                "notifySuccessUrl",
                "overrideAnalyticsId"
            ]
        }
